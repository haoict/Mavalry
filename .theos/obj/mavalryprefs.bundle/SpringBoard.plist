<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- general cell -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>General</string>
		</dict>
		<!-- enable/disable button -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ajaidan.mavalryprefs</string>
			<key>key</key>
			<string>wantsHiddenLabels</string>
			<key>label</key>
			<string>Hide Icon Labels</string>
			<key>PostNotification</key>
			<string>com.ajaidan.mavalryprefs.settingschanged</string>
		</dict>

		<!-- enable/disable page dots button -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ajaidan.mavalryprefs</string>
			<key>key</key>
			<string>wantsHiddenPageDots</string>
			<key>label</key>
			<string>Hide Page Dots</string>
			<key>PostNotification</key>
			<string>com.ajaidan.mavalryprefs.settingschanged</string>
		</dict>

		<!-- enable/disable transparent dock button -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ajaidan.mavalryprefs</string>
			<key>key</key>
			<string>wantsTransparentDock</string>
			<key>label</key>
			<string>Transparent Dock</string>
			<key>PostNotification</key>
			<string>com.ajaidan.mavalryprefs.settingschanged</string>
		</dict>

		<!-- enable/disable hiding wantsHomeBar -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ajaidan.mavalryprefs</string>
			<key>key</key>
			<string>wantsHomeBar</string>
			<key>label</key>
			<string>Hide wantsHomeBar</string>
			<key>PostNotification</key>
			<string>com.ajaidan.mavalryprefs.settingschanged</string>
		</dict>
	</array>
	<key>title</key>
	<string>Mavalry</string>
</dict>
</plist>
