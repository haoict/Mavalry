<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<!-- banner -->
        <dict>
            <key>cell</key>
            <string>PSGroupCell</string>
            <key>headerCellClass</key>
            <string>BRUHRootHeaderView</string>
            <key>icon</key>
            <string>PrefHeader.png</string>
        </dict>

		<!-- general enable/ disable switch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.ajaidan.mavalryprefs</string>
			<key>key</key>
			<string>isEnabled</string>
			<key>label</key>
			<string>Enabled</string>
			<key>PostNotification</key>
			<string>com.ajaidan.mavalryprefs.settingschanged</string>
		</dict>

		<!-- space between main switch and stuff below -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string></string>
		</dict>

		<!-- SpringBoard subPref -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SB</string>
			<key>label</key>
			<string>SpringBoard</string>
		</dict>

		<!--LockScreen subPref -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>Lockscreen</string>
			<key>label</key>
			<string>Lockscreen</string>
		</dict>

		<!-- CC subPref -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>CC</string>
			<key>label</key>
			<string>Control Center</string>
		</dict>

		<!-- Applications subPref -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>Applications</string>
			<key>label</key>
			<string>Applications</string>
		</dict>

		<!-- Applications subPref -->
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>Haptics</string>
			<key>label</key>
			<string>Haptics</string>
		</dict>

		<!-- footer -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>About</string>
			<key>footerText</key>
			<string>Tweak made with ❤️ by ajaidan0.</string>
		</dict>

		<!-- source code button -->
		<dict>
			<key>action</key>
			<string>sourceLink</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>icon</key>
			<string>GitHub</string>
			<key>label</key>
			<string>Source Code</string>
		</dict>
		<dict>
			<key>action</key>
			<string>donate</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>icon</key>
			<string>Donate</string>
			<key>label</key>
			<string>Donate</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string></string>
			<key>footerText</key>
			<string>Icons and Volume Haptic made with ❤️ by @samoht#9277.</string>
		</dict>

	</array>
	<key>title</key>
	<string>Mavalry</string>
